#include <stdio.h>
#include <stdlib.h>

int main()
{
   int n1, n2, n3, l, s;

    printf("Input Your 3 Digits\n");
    scanf("%d %d %d", &n1, &n2, &n3);

    l = n1;
    s = n1;

    if(n2>l)
        l = n2;
    if(n3>l)
        l = n3;


    if(n2<s)
        n2 = s;
    if(n3<s)
        n3 = s;

    printf("Largest Number is %d and the Smallest is %d", l, s);
    
}
