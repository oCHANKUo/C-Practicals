#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1, n2;

    printf("Input Your 2 Numbers: ");
    scanf("%d %d", &n1, &n2);

    if(n1%n2 == 0)
        printf("%d is a multiple of %d", n1, n2);
    else if (n1<n2)
        printf("%d is smaller than %d", n1, n2);
    else
        printf("%d is not a multiple of %d", n2, n1);
   
}
