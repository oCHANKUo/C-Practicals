#include <stdio.h>
#include <stdlib.h>

int main() {

    int matrix1[3][3] = {
        {3, 2, 4},
        {1, 4, 6},
        {4, 3, 2}
    };

     int matrix2[3][3] = {
        {2, 6, 3},
        {4, 3, 2},
        {5, 1, 7}
    };

    int sum[3][3], i, j;

    for(i = 0; i<3; i++)
    {
        for(int j=0; j<3; j++)
        {
            sum[i][j] = matrix1[i][j] + matrix2[i][j];
        }
    }

    printf("sum: \n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d ", sum[i][j]);
        }
        printf("\n");
    }


    return 0;
}

