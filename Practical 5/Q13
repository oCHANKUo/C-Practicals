#include <stdio.h>

int main() {
    int userInput;
    int sum = 0;

    printf("Enter numbers to add (Enter -1 to stop):\n");

    while (1) {
        scanf("%d", &userInput);
        if (userInput == -1) {
            break;
        }
        sum += userInput;
    }

    printf("Sum of the numbers: %d\n", sum);

    return 0;
}
