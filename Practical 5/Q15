#include <stdio.h>

int main() {
    int arr[10];
    int countEven = 0;

    printf("Enter 10 integers:\n");

    for (int i = 0; i < 10; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] % 2 == 0) {
            countEven++;
        }
    }

    printf("The array you entered is:\n");
    for (int i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }

    printf("\n");
    printf("Count of even numbers: %d\n", countEven);

    return 0;
}
